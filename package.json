    "@truffle/expect": "^0.0.15",
    "@vue-stripe/vue-stripe": "^4.2.5",
    "abi-decoder": "^2.3.0",
    "axios": "^0.21.1",
    "chokidar": "^3.5.0",
    "copy-webpack-plugin": "^6.3.2",
    "core-js": "^3.6.5",
    "ethers": "^5.6.8",
    "firebase": "^8.2.4",
    "firebaseui": "^4.8.0",
    "ipaddr.js": "^2.0.0",
    "logrocket": "^1.0.14",
    "momentjs": "^2.0.0",
    "pusher-js": "^7.1.1-beta",
    "stripe": "^8.176.0",
    "v-stripe-elements": "^1.2.0",
    "vue": "^2.6.11",
    "vue-json-pretty": "^1.8.2",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.4.9",
    "vuefire": "^2.2.5",
    "vuetify": "^2.3.9",
    "vuex": "^3.6.0",
    "vuexfire": "^3.2.5",
    "web3": "^1.3.1",
    "web3-providers-ws": "^1.3.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.3.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "firebase-admin": "^9.9.0",
    "firestore-jest-mock": "^0.13.0",
    "flush-promises": "^1.0.2",
    "node-fetch": "^2.6.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue-cli-plugin-vuetify": "~2.0.8",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-multi-spaces": "error",
      "no-trailing-spaces": "error",
      "eol-last": "error"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
